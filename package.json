        "nookies": "^2.5.2",
        "pm2": "^5.4.2",
        "prisma": "5.21.1",
        "react": "19.0.0-rc-65a56d0e-20241020",
        "react-dom": "19.0.0-rc-65a56d0e-20241020",
        "react-hook-form": "^7.56.3",
        "react-icons": "^5.5.0",
        "react-screen-wake-lock": "^3.1.1",
